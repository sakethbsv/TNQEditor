var sowl$ = new OWLstart();

function OWLstart() {

    this.init = function() {
	function setOWLreport(html) {
	    var doc = window.document.getElementById("owl-content").contentDocument;
	    $(doc).find("#owlReport").html(html);
	}
	setOWLreport(window.arguments[0]);
    }

}
